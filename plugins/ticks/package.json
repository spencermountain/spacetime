  },
  "devDependencies": {
    "@babel/core": "7.6.3",
    "@babel/preset-env": "7.6.3",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "budo": "^11.6.3",
    "derequire": "^2.0.6",
    "htm": "2.2.1",
    "rollup": "1.23.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.1.2",
    "somehow": "0.3.1",
    "somehow-input": "0.0.9",
    "spacetime": "6.1.0",
